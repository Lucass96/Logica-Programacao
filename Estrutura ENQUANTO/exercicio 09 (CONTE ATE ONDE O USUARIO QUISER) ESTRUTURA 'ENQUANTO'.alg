algoritmo "Conte_ate_onde_o_usuario_quiser"
var
   contador, N: Inteiro
inicio
      Escreval("Digite um numero: ")
      Leia(N)
      contador <- 0
      Enquanto (contador <= N) faca
               Escreval(contador)
               contador <- contador + 1
      FimEnquanto
      Escreval("Terminei de contar.")
fimalgoritmo
